/******************************************************************************
Reverse an array
*******************************************************************************/
import java.util.*;
public class Main
{
	public static void main(String[] args) 
	{
		int a[] = {1, 2, 4, 6, 7, 5};
		int n = a.length;
		int ans[]=new int[n];
		for(int i=n-1;i>=0;i--)
		{
		    ans[n-i-1] = a[i];
		}
		for(int i=0;i<n;i++)
		{
		    System.out.print(ans[i]+"\t");
		}
	}
}
