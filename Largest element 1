/******************************************************************************
Largest element in an array 
*******************************************************************************/
import java.util.*;
public class Main
{
	public static void main(String[] args) 
	{
		int a[] = {4,5,3,1};
		int n = a.length;
		int max=a[0];
		for(int i=0;i<n;i++)
		{
		    if (a[i]>max)
		    max=a[i];
		}
		System.out.println(max);
	}
}
