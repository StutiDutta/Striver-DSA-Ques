/******************************************************************************
Smallest element in an array 1
*******************************************************************************/

public class Main
{
	public static void main(String[] args) 
	{
		int a[] = {4,5,3,1};
		int n = a.length;
		int min=a[0];
		for(int i=0;i<n;i++)
		{
		    if (a[i]<min)
		    min=a[i];
		}
		System.out.println(min);
	}
}
